block pageVariables
doctype html
html(lang="en")
    head
        title= pageTitle
        include frames/metahead
        block pageHead
    body
        div(class="mainwrapper" id="awes-app")
            div(class="frame")
                include frames/aside_docs
                div(class="frame__right")
                    include frames/header_docs
                    div.frame__content
                        block pageContent
                            p Content Block
