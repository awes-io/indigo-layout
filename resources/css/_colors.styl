  &-titlenav
    a
      color var(--tc_link_semi)
      &:hover, em
        color var(--tc_link_semi_active)
      &.active
        color var(--tc_link)
  &-caption
    color var(--tc_text_caption)
  &-blcode
    color cl_white
    background-color cl_black
  ol
    li
      &:before
        color var(--tc_text_caption)
  &-dottedlink
    color var(--tc_link_semi)
    border-color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)
      border-color var(--tc_link_semi_active)
  &-pagetitle
    color var(--tc_text)
  &-hyplink
    color var(--tc_text_caption)
    &:hover
      color var(--tc_link)



.chart
  &-icon
    i
      border-color var(--tc_ui_bg)

.chart-box
  box-shadow var(--tc_ui_shadow)
  background-color var(--tc_ui_bg)
  color var(--tc_text)
  & &__icon
    color var(--tc_text_caption)
  & &__list
    a
      color var(--tc_link_semi)
      &:hover
        color var(--tc_link_semi_active)
      &.active
        color var(--tc_link_semi_active)
        border-color var(--tc_link_semi_active)
  & &__mrlink
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)
  & &__info
    &-more
      color var(--tc_link_semi)
