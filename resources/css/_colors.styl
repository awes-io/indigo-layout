      background-color cl_black
    &_1
      &:before, &:after
        background-color cl_morning
    &_2
      &:before, &:after
        background-color cl_dream
    &_3
      &:before, &:after
        background-color cl_darkdrean
    &_4
      &:before, &:after
        background-color cl_japanshine
    &_5
      &:before, &:after
        background-color cl_heartlife



.tf
