
body
  color var(--tc_text)
  background-color var(--tc_ui_bg)

blockquote
  background-color var(--tc_ui_bg)
  box-shadow var(--tc_ui_shadow)
  &:before
    color var(--tc_text_caption)
    opacity .5
  span
    color var(--tc_text_caption)

em
  background-color transparent

s
  color var(--tc_text_caption)

.frame__content
  ol
    li
      &:before
        color var(--tc_text_caption)
a
  color var(--tc_link)
  &:hover
    color cl_skydark

.awes-chat
  & &__threads
    background-color var(--tc_ui_bg_semi)
  & &__smiles
    background-color var(--tc_ui_shadow-middle)
  & &__thread
    border-color var(--tc_border)
    background-color var(--tc_ui_bg_semi)
    &:hover
      background-color var(--tc_ui_bg_semi_v)
    &-time
      color var(--tc_text_caption)
    &.active
      background-color var(--tc_ui_bg)
  & &__message
    &-time
      color var(--tc_text_caption)
  & &__form
    border-color var(--tc_border)
    background-color var(--tc_ui_bg)
    &-btn
      color var(--tc_link_semi)
      &:hover
        color var(--tc_link_semi_active)
      &:focus
        background-color var(--tc_ui_bg_semi)

.awes-status
  background-color var(--tc_ui_bg_semi_d)
  &_blue
    background-color cl_morning
  &_green
    background-color cl_grass

.infbl
  & &_icon
    color var(--tc_text_caption)
  & &__caption
    color var(--tc_text_caption)

.static-page
  background-color var(--tc_ui_bg_semi)
  & &__head
    background-image var(--tc_aside_gradient)
    color var(--tc_invert)
  & &__content
    background-color var(--tc_ui_bg)
  & &__search-link
    color var(--tc_aside_text)
    &:hover
      color var(--tc_link)
  & &__title
    color var(--tc_aside_text)
  & &__footer
    &-logo
      color var(--tc_text_caption)
      a
        color var(--tc_text_caption)
        &:hover
          color var(--tc_link)
      .icon
        color var(--tc_link)


.history-list
  li
    color var(--tc_text_caption)
    &:before
      box-shadow 0px 0px 0px 2px var(--tc_text_caption) inset
    &.active
      color cl_grass
      &:before
        background-color cl_grass
        box-shadow none
      &:after
        background-color  var(--tc_border)
    &.current
      color cl_grass
      &:before
        box-shadow 0px 0px 0px 2px cl_grass inset

.project-page
  & &__logo
    box-shadow var(--tc_shadow_block)
  & &__progress
    background-color var(--tc_border)
    span
      background-color cl_grass
      color cl_grass
      &:after
        background-color cl_grass





.action-menu
  & &__click
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)

.ava-text
  .icon
    background-color var(--tc_ui_bg_darken)
    color var(--tc_text_caption)

.novatar
  background-color var(--tc_ui_bg_darken)
  color var(--tc_text_caption)

.number-field
  & &__field
    background-color var(--tc_ui_bg)
    border 1px solid var(--tc_border)
  & &__plus, & &__minus
    border 1px solid var(--tc_border)
    color var(--tc_ui_btn_light_text)
    background-color var(--tc_ui_btn_grey)
    &:hover
      background-color var(--tc_ui_hover)
      color var(--tc_ui_hover_text)
      border 1px solid var(--tc_ui_hover)

.figure-loader
  & &__img
    background-color var(--tc_ui_bg)
  & &__remove
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)

.form-title
  color var(--tc_text)
  .icon
    color var(--tc_text_caption)

.ui-title
  color var(--tc_text)



.form-spoiler
  & &__title
    color var(--tc_link_black)
    &:hover
      color var(--tc_link)
  &:before
    background-color var(--tc_border_light)
  &:after
    border-color var(--tc_border_night)

.vue-dialog
  .vue-dialog-buttons
    .vue-dialog-button
      &:nth-child(2)
        border-top 1px solid var(--tc_border_light)
        background-color var(--tc_ui_bg) !important
        color cl_asphalt !important
        &:hover
          {lt} &
            background-color var(--tc_ui_bg_d) !important
          {dt} &
            background-color var(--tc_ui_bg_l) !important
          color cl_asphalt






.vue-dialog-buttons
  .vue-dialog-button
    background-color cl_grass !important
    color cl_white !important
    &:hover
      background-color darken(cl_grass, 10%) !important
    &:nth-child(2)
      background-color cl_white !important
      color cl_asphalt !important
      &:hover
        background-color darken(cl_white, 5%) !important

.hfilter
  & &__wrap
    background-color var(--tc_ui_bg)
    min-height 100%

.overflow-window
  background-color var(--tc_ui_bg)
  color var(--tc_text)
  & &__link
    color var(--tc_link_black)
    &_red
      color cl_warn
    &:hover
      background-color var(--tc_ui_btn_light)
  & &__title
    color var(--tc_text_caption)
  & &__close
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)

.filter
  & &__slink
    color var(--tc_link_semi)
    .icon
      color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)
      .icon
        color var(--tc_link_semi_active)
      &:after
        border-top-color var(--tc_link_semi_active)
  & &__checkbox
    &-label
      border-color var(--tc_border)
      color var(--tc_link_semi)
      &:hover
        color var(--tc_link_semi_active)
    &-datapicker
      color var(--tc_link_semi)
      &:focus
        color var(--tc_link_semi_active)
    & ^[0]__checkbox-hInput:checked + .filter__checkbox-label
      background-color var(--tc_ui_bg)
      color var(--tc_link_semi_active)
      .filter__checkbox-datapicker
        background-color var(--tc_ui_bg)
    &-cell
      &_active
        & ^[0]__checkbox-label
          background-color var(--tc_ui_bg)
          color var(--uiElement_active)
        & ^[0]__checkbox-datapicker
          background-color var(--tc_ui_bg)
  &__form-builder
    background-color var(--tc_ui_bg)

.price-table
  // border 1px solid var(--tc_border)
  padding-left 1px
  & &__cell
    border 1px solid var(--tc_border)
    background-color var(--tc_ui_bg)
    &.active
      border 1px solid cl_grass
  & &__list
    li
      border-bottom 1px solid var(--tc_border_dark)
      color var(--tc_text_caption)
      &:last-child
        border-bottom 0
  & &__spoiler
    & ^[0]__list
      li
        &:last-child
          border-bottom 1px solid var(--tc_border_dark)
  & &__caption
    color var(--tc_text_caption)
    &_dark
      color var(--tc_text)
  & &__moreinfo
    color var(--tc_link_dark)
    &:hover
      text-decoration underline
  & &--cancel
    &:hover
      color cl_red

.paycard
  & &__current
    color var(--tc_text)
    &-caption
      color var(--tc_text_caption)
  & &__info-text
    color var(--tc_text_caption)
  & &__wrap
    color cl_white
    background-image var(--tc_gradient_1)
    box-shadow var(--tc_shadow_blue)
  & &__select
    color cl_white
    &:hover
      color cl_white !important
  &_red
    & ^[0]__wrap
      background-image var(--tc_gradient_2)
      box-shadow var(--tc_shadow_red)
  &_white
    & ^[0]__wrap
        box-shadow var(--tc_shadow_gblue)
        background-image none
        background-color var(--tc_ui_bg)
    & ^[0]__select
        color var(--tc_link_semi)
  & &__paylogo
    figcaption
      color var(--tc_text)
  & &__hover
    background-color var(--tc_ui_bgopacity)
  &:hover
    & ^[0]__select
      color var(--tc_link_semi)

@media (pointer coarse)
  .paycard
    & &__hover
      background-color transparent
    & &--btn
      color var(--tc_link_semi) !important
      background-color transparent !important
    & &__select
      color var(--tc_link_semi) !important





.radiobox
  color var(--tc_link_semi)
  & &__icon
    border 1px solid var(--tc_border)
    &:before
      opacity 0
  & ^[0]__field:checked + .radiobox__content
    & ^[0]__text
      color var(--tc_link_semi_active)
    & ^[0]__icon
      background-color cl_grass
      border-color cl_grass
      color cl_white
      &:before
        opacity 1
        background-color cl_white
  & ^[0]__field:focus + .radiobox__content
    & ^[0]__icon
      border 1px solid var(--tc_ui_focus)

.box-checker
  .fc-radio .fc-radio__box.is-checked
    background-color var(--tc_ui_bg)
    color var(--tc_text)
    z-index 2
    position relative
    box-shadow 0 0 5px rgba(0, 0, 0, 0.1)
    border 1px solid cl_grass
    &:not(:first-child)
      margin-left -1px
    .box-checker__caption
      color var(--tc_text_caption)



.icn-dot
  background-color cl_warn


.theme-switcher
  &__label
    color var(--tc_text_caption)
    tr(0.25)
  &__switcher
    {lt} &
        background-color var(--tc_ui_darkbg)
    {dt} &
        background-color var(--tc_ui_darkbg_d)
    &:before
        background-color cl_white
        box-shadow 0px 0px 5px var(--tc_ui_darkbg_o)
  &.is-active .theme-switcher__switcher
      background-color cl_grass
  &:hover
    & ^[0]__label
      color var(--tc_text)









.loading-inline, .loading-block
  &:before
    background-color var(--tc_ui_bg)

.btn
  color var(--tc_btn_text)
  background-color var(--tc_btn_bg)
  &:hover, &:focus
    background-color var(--tc_btn_bg_active)
    color var(--tc_btn_text)
  &_white
    background-color var(--tc_btn_tr_bg)
    color var(--tc_btn_tr_text)
    &:hover, &:focus
      background-color var(--tc_btn_tr_bg_d)
      color var(--tc_btn_tr_text)
  &_transparent
    background-color transparent
    color var(--tc_btn_tr_text)
    &:hover, &:focus
      background-color var(--tc_ui_bg_darken_s)
  &_sh
    box-shadow 1px 2px 3px var(--tc_btn_bg_shadow);
  &_darkred
    background-color transparent
    color cl_white
    &:hover, &:focus
      color cl_warn
      background-color transparent
  &_greylink
    background-color transparent
    color var(--tc_link_semi)
    &:hover, &:focus
      background-color transparent
      color var(--tc_link)
  &.loading-inline
    &:hover, &:focus
      background-color cl_grass
  &[disabled="disabled"]
    background-color var(--tc_ui_darkbg) !important
    pointer-events none
    opacity 0.5



.pager
  & &__caption
    color var(--tc_link_semi)
  & &__shows
    &-link
      color var(--tc_link_semi)
      &:hover
        color var(--tc_link_semi_active)
        &:after
          border-top-color var(--tc_link_semi_active)
  & &__link
    border 1px solid var(--tc_border)
    color var(--tc_link_semi)
    &_active
      background-color var(--tc_ui_bg)
      color var(--tc_link_semi_active)
    &:hover
      color var(--tc_link_semi_active)
  & &__spacer
    border 1px solid var(--tc_border)
    color var(--tc_link_semi)
  & &__arr-left, & &__arr-right
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)

.actions-overflow
  & &__icon
    border-left 1px solid var(--tc_border_light)
    border-bottom 1px solid var(--tc_border_light)
  & &__link
    color var(--tc_link_black)
    &_red
      color cl_warn
    &:hover
      background-color var(--tc_ui_btn_light)
  & &__title
    color var(--tc_text_caption)
  & &__actions
    background-color var(--tc_ui_bg)

.tooltip
  background-color var(--tc_tooltip_bg)
  &:before
    border-top 4px solid var(--tc_tooltip_bg)
  &[x-placement="right"]
    &:before
      border-top 4px solid transparent
      border-right 4px solid var(--tc_tooltip_bg)
  &.theme-error
    background-color cl_warn
    color var(--tc_tooltip_text)
    &:before
      border-top-color cl_warn
    &[x-placement="right"]
      &:before
        border-top 4px solid transparent
        border-right 4px solid cl_warn

.notifications
  .vue-notification
    background-color cl_sky !important
    {lt} &
      box-shadow 3px 4px 20px alpha(cl_sky, 0.5) !important
    {dt} &
      box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important
    &__animation
      background-color darken(cl_sky, 15%) !important
    &.success
      background-color cl_grass !important
      box-shadow 3px 4px 20px alpha(cl_salad, 0.5) !important
      {lt} &
        box-shadow 3px 4px 20px alpha(cl_salad, 0.5) !important
      {dt} &
        box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important
      .vue-notification__animation
        background-color darken(cl_salad, 15%) !important
      .vue-notification__btn:hover
        color cl_yellow !important
    &.error
      background-color cl_warn !important
      {lt} &
        box-shadow 3px 4px 20px alpha(cl_warn, 0.5) !important
      {dt} &
        box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important
      .vue-notification__animation
        background-color darken(cl_warn, 15%) !important
      .vue-notification__btn:hover
        color cl_warn !important
    &.warn
      background-color cl_yellow !important
      {lt} &
        box-shadow 3px 4px 20px alpha(cl_yellow, 0.5) !important
      {dt} &
        box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important
      .vue-notification__animation
        background-color darken(cl_yellow, 15%) !important
      .vue-notification__btn:hover
        color cl_yellow !important
  &_header
    .vue-notification
      &__btn
        color cl_white
        border 1px solid alpha(cl_white, 0.5)
        &:hover
          background-color cl_white
          color var(--tc_link)
      &__close
        color cl_white
  &_rb
    .vue-notification
      &__wrap
        padding-left 30px
      &__close
        color cl_white
      &__left-icon
        color cl_white

.toasted
    &.success
        background-color cl_grass !important
        box-shadow 3px 4px 20px alpha(cl_salad, 0.5) !important
        {lt} &
          box-shadow 3px 4px 20px alpha(cl_salad, 0.5) !important
        {dt} &
          box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important
    &.error
      background-color cl_warn !important
      {lt} &
        box-shadow 3px 4px 20px alpha(cl_warn, 0.5) !important
      {dt} &
        box-shadow 3px 4px 20px alpha(cl_black, 0.5) !important



.flex-list
  & &__caption
    color var(--tc_text_caption)
  & &__title
    color var(--tc_text)

.text-caption
  color var(--tc_text_caption)

.blue-link
  color var(--tc_link)

.upper-link
  color var(--tc_text_caption)
  &:hover
    color var(--tc_link)

table:not(.int-table__table):not(.hljs-ln)
  background-color var(--tc_ui_bg)
  th
    background-color var(--tc_ui_bg_darken)
    color var(--tc_text_caption)
  td
    border-bottom 1px solid var(--tc_border_dark)
  tr
    &:hover
      background-color var(--tc_ui_bg_semi)

.int-table
  & &__footer
    background-color var(--tc_ui_bg_darken)
  & &__drag-icon
    .icon
      color var(--tc_text_caption)
  & &__flink
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)
  & &__fcaption
    color var(--tc_text)
  & &__header
    border 1px solid var(--tc_border)
  & &__count
    span
      color var(--tc_text_caption)
  & &__table
    background-color var(--tc_ui_bg)
    th
      background-color var(--tc_ui_bg_darken)
      color var(--tc_text_caption)
    td
      border-bottom 1px solid var(--tc_border_dark)
    tr
      &:hover
        background-color var(--tc_ui_bg_semi)
    tr.active:hover
      background-color transparent

    tbody
      box-shadow var(--tc_ui_shadow)
  & &__show
    color var(--tc_text_caption)
  & &__status
    &-icon
      color var(--tc_text_caption)
  & &__link, a
    color var(--tc_link)
  & &__caption
    color var(--tc_text_caption)
  & &__status
    border 1px solid var(--tc_border)
    color var(--tc_text_caption)
    &_stop
      background-color cl_warn
      color cl_white
      border 1px solid cl_warn
    &_inproggress
      border 1px solid cl_grass
      color cl_grass
    &_sleep
      border 1px solid var(--tc_link)
      color var(--tc_link)
  & &__controll
    color var(--tc_link_semi)
    &:hover, &.active
      color var(--tc_link_semi_active)
    &-tab
      @media maw[dmd]
        border-left 1px solid var(--tc_border)
        &.active
          background-color var(--tc_border_semi)
  & &__block
    &.active
      & ^[0]__controll-tab
        @media maw[dmd]
            background-color var(--tc_ui_bg_semi)
            border-bottom 1px solid var(--tc_border)


.user-menu
  & &__list
    box-shadow var(--tc_ui_context_menu_shadow)
    background-color var(--tc_ui_context_menu_bg)



.helper
  & &__block
    box-shadow var(--tc_ui_context_menu_shadow)
    background-color var(--tc_ui_context_menu_bg)
  & &__title
      &_btn
        span
          &:after
            border-top-color var(--tc_text)
  & &__close
    color var(--tc_link_semi)
    &:hover
      color var(--tc_link_semi_active)
  & &__head, & &__stepper
    border-color var(--tc_border)
  .nav-tabs
      background-color var(--tc_ui_bg_semi)
      .tabs__link
        color var(--tc_link_semi)
      .active_tab
        background-color var(--tc_ui_bg)
        color var(--tc_link_semi_active)
        &:after
          border-top-color var(--tc_text)
        @media maw[mmd]
          border-color var(--tc_border)




.head-context
  & &__main
    border-color var(--tc_border)
    background-color var(--tc_ui_bg)
    &:hover
        background-color: var(--tc_ui_bg_semi)
  & &__hidden
    border-color var(--tc_border)
    background-color var(--tc_ui_bg)
    &-btn
      color var(--tc_link_semi)
      &:hover
        background-color: var(--tc_ui_bg_semi)

.context-menu
  &__overlay
    background-color var(--tc_ui_shadow-overflow)

  &__list
    box-shadow var(--tc_ui_context_menu_shadow)
    background-color var(--tc_ui_context_menu_bg)

  &__toggler-default
    {lt} &
      background-color var(--tc_ui_bg_semi_v)
    {dt} &
      background-color var(--tc_ui_bg_semi_s)
    &:hover
      {lt} &
        background-color var(--tc_ui_bg_semi_s)
      {dt} &
        background-color var(--tc_ui_bg_semi_d)
    circle
      fill var(--tc_text)


  &.is-opened
    .context-menu__toggler-default
        background-color var(--tc_ui_context_menu_border)

  &.is-top-side
    .context-menu__list
      box-shadow var(--tc_ui_context_menu_shadow_top)

.cm-item
  &.is-separator
    border-bottom 1px solid var(--tc_ui_context_menu_border)

  &__link,
  &__button
    color var(--tc_link_semi)

    &:hover,
    &:focus
      color var(--tc_link_semi_active)
      background-color var(--tc_ui_context_menu_hover_bg)

    &.is-danger
      color cl_warn

    &.is-active
      border-bottom-color var(--tc_ui_context_menu_hover_bg)
      color var(--tc_link_semi_active)

  &__panel
    border-bottom 1px solid var(--tc_ui_bg_darken)

    &.is-secondary
      background-color var(--tc_page_bg)

.chart
  &-icon
    &:before
      background-color cl_black
      border-color cl_white
