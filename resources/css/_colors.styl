      color cl_warn
  &_error
    & ^[0]__icon
      color cl_warn

.cl_caption
  color var(--tc_text_caption)

.cl
  &_red
    color cl_warn
  &_green
    color cl_grass


.justify-list
  color var(--tc_text)
  box-shadow var(--tc_ui_shadow)
  background-color var(--tc_ui_bg)
  a
    color var(--tc_link)
  li
    border-color var(--tc_border_light)

.j-table
  color var(--tc_text)
  box-shadow var(--tc_ui_shadow)
  background-color var(--tc_ui_bg)
  & &__nd
    &-caption
      color var(--tc_text_caption)
    &-icon
      color var(--tc_text_caption)
  a
    color var(--tc_link)
  & &__rate
    color var(--tc_text_caption)
  td
    border-color var(--tc_border_light)
    @media maw[mmd]
      border-bottom 0
  tr
    @media maw[mmd]
      border-color var(--tc_border_light)

.dashboard
  & &__tabs
    box-shadow var(--tc_ui_shadow)
    .justify-list
      box-shadow none
    .tabs-component
      &-tab
        border-color var(--tc_border_light)
        font-size f_normal
        a
          color var(--tc_text_caption)
          &:hover
            color var(--tc_text)
        &.is-active
          background-color var(--tc_ui_bg)
          a
            color var(--tc_text)
            &:hover
              color var(--tc_text)

.header-notification, header-notification
  &.error, &[type="error"]
    background-color cl_warn
    color cl_white
    & ^[0]__close
      color cl_white

.fb
  &__input-domain
    color var(--tc_text_caption)


.profile-wrap
  & &__ava
    background-color var(--tc_ui_bg_darken)
    &-noavatar
      color var(--tc_text_caption)


.login-page
  & &__left
    background-color var(--tc_ui_bg)
  & &__spacer
    color var(--tc_text_caption)
    i
      background-color var(--tc_ui_bg)
      &:before, &:after
        background-color var(--tc_border)
  & &__logo
    color var(--tc_link)
    &:hover
      color var(--tc_color_main_l)
    &_white
      color cl_white
      @media maw[mlg]
        {lt} &
          color var(--tc_color_main)
        {dt} &
          color var(--tc_link)
      &:hover
        color cl_sky

  & &__sbox
    background-color var(--tc_ui_bg)
  & &__full
    @media maw[mlg]
      background-color var(--tc_ui_bg)


::-webkit-scrollbar-button
  background-color var(--tc_border)

::-webkit-scrollbar-track
  background-color var(--tc_border)

::-webkit-scrollbar-corner
  background-color cl_red

::-webkit-scrollbar
  background-color: var(--tc_border)

::-webkit-resizer
  background-color cl_red

::-webkit-scrollbar-thumb
  background-color var(--tc_scroll_thumb)

::-webkit-scrollbar-thumb:hover
  background-color var(--tc_scroll_thumb_active)

.float-icns
    & &__icon
      &_helper
        box-shadow var(--tc_shadow_blur)
        background-color cl_skydark
        color cl_white
        &:hover
          background-color cl_sky







.todolist
  background-color var(--tc_ui_bg)
  color var(--tc_text)
  & &__head
    border-bottom 1px solid var(--tc_border_dark)
  & &__block
    border-bottom 1px solid var(--tc_border_dark)
    &-add
      color var(--tc_link_black)
      border 1px solid var(--tc_border_dark)
    &-title
      color var(--tc_text)
      span
        color var(--tc_text_caption)
      &:before
        color var(--tc_text_caption)
      &:hover
        background-color var(--tc_ui_btn_light)
  & &__plus
    color var(--tc_link_caption)
    &:hover
      color var(--tc_link_black)

.profile-wrap
  & &__address
    &-blacklink
      color var(--tc_link_black)
      &:hover
        color var(--tc_link)

.project-block
  background-color var(--tc_ui_bg)
  &:hover
    box-shadow var(--tc_ui_shadow)
  & &__time
    color var(--tc_text_caption)
  & &__title
    color var(--tc_link_black)
    &:hover
      color var(--tc_link_black_active)
  & &__type
    color var(--tc_text_caption)
  & &__caption
    color var(--tc_text_caption)
  & &__ava
    border 1px solid var(--tc_ui_bg)
  & &__percent
    border-bottom 1px solid var(--tc_border)
    color var(--tc_text_caption)
  & &__status
    border 1px solid var(--tc_border_prj)
    color var(--tc_text_caption)
    &_inproggress
      border 1px solid cl_salad
      color cl_salad
    &_stop
      border 1px solid cl_warn
      background-color cl_warn
      color cl_white
    &_sleep
      border 1px solid var(--tc_link)
      color var(--tc_link)
  &_inproggress
    & ^[0]__caption
      color cl_salad
    & ^[0]__percent
      border-bottom 1px solid cl_salad
      color cl_salad
  &_stop
    & ^[0]__caption
      color cl_warn


.user-menu
  .context-menu__dropdown
    color var(--tc_text)
    background-color var(--tc_header_bg)
  & &__desc
    color var(--tc_text_caption)

.progress
  color var(--tc_text_caption)
  span
    color cl_grass
  progress
    &::-webkit-progress-value
      background-color cl_grass

    &::-moz-progress-bar
      background-color cl_grass
  &.is-danger
    span
      color cl_warn

    progress
      &::-webkit-progress-value
        background-color cl_warn

      &::-moz-progress-bar
        background-color cl_warn

.wave
  background-color: var(--tc_ui_wave)