tab-builder
    fl()
    position relative
    box-shadow inset 0 -1px 0px 0px var(--tc_border)
    // margin-left -40px
    // margin-right -40px
    // padding-left 40px
    // padding-right 40px
    // margin-top -27px !important
    height 50px
    display block
    margin-bottom 25px
    position relative
    & > *
        display none
    &:before
        display block
        // absolute left 40px top 50%
        absolute left 0 top 50%
        margin-top -10px
        // top 50%
        max-width 500px
        size 100% 20px 
        phLineBg(--tc_ui_bg_darken)
        content ''
        
