extends includes/layout.pug

block pageVariables
    - var pageTitle = "Typography"

block pageHead
    // there is can be head meta or sctipts
    script(src="https://storage.awes.io/680a7d07f89b94e7fc83be657a2daa27/awes-io/form-builder/v0.x.x/js/main.js")
    script(src="https://storage.awes.io/680a7d07f89b94e7fc83be657a2daa27/awes-io/modal/v0.1.0/js/main.js")


block pageContent
    button.btn(@click="AWES.emit('modal::default.open', 'form')") Default modal
    br
    button.btn(@click="AWES.emit('modal::fullscreen.open')") Fullscreen modal
    br
    button.btn(@click="AWES.emit('modal::modalform.open' )") Modal form modal
    br
    br

    modal-window(name='modalform', title="Modal form", data-awes='checkOpenModal')
        form-builder(url="valera", method='PATCH', store-data='content')
            template(slot-scope='{ fields }')
                fb-checkbox(name='theme_dark', theme='s2', label="{{_p('managers.form.theme_dark', 'Dark theme')}}")
                fb-input(name='email', type='email', label="{{_p('general.email', 'E-Mail')}}", :disabled='!fields.active')
                fb-input(name='profile_manager.first_name', label="{{_p('managers.form.first_name', 'First Name')}} *", :disabled='!fields.active', focus='')
                fb-input(name='profile_manager.last_name', label="{{_p('managers.form.last_name', 'Last Name')}}", :disabled='!fields.active')
                fb-checkbox(name='active', theme='s2', label="{{_p('managers.form.active', 'Active manager')}}")
                fb-input(name='profile_manager.position', label="{{_p('managers.form.position', 'Position')}}", :disabled='!fields.active')
                fb-input(name='language_id', label="{{_p('managers.form.lang_id', 'Interface languange')}}", :disabled='!fields.active')
                fb-multi-block(name='phones', label="{{_p('managers.form.add_phone', 'Add phone')}}", :disabled='!fields.active')
                    template(slot-scope='{ id }')
                        fb-input(name='phone', label="{{_p('general.phone', 'Phone')}}", :id='id')
                fb-input(name='profile_manager.fax', label="{{_p('general.fax', 'Fax')}}", :disabled='!fields.active')
                fb-multi-block(name='socials', label="{{_p('managers.form.add_social', 'Add social network')}}", :disabled='!fields.active')
                    template(slot-scope='{ id }')
                        fb-input(name='social', label="{{_p('general.social', 'Social network')}}", :id='id')
             

